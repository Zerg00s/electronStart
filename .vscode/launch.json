{
    "version": "0.2.0",
    "configurations": [
        {
"name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/main.js", // we will run gulp.js when hitting F5
            "stopOnEntry": false,
            "args": [], // name of the gulp task that will be run when we click F5 in VS Code
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "runtimeArgs": [
                ".",
                "--enable-logging" ],
            "env": {
                "NODE_ENV": "development"
            },
            "console":"integratedTerminal",
            "sourceMaps": false,
            "outFiles": []
        }
    ]
}